<?xml version="1.0" ?>
<launch>
	<!-- <param name="robot_description" command="$(find xacro)/xacro.py '$(find robinion_description)/urdf/robinion.xacro'" /> -->
	<param name="robot_description" command="$(find xacro)/xacro '$(find gripper_description)/urdf/gripper_view.xacro'" />

	<!-- send joint values -->
	<node pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" name="joint_state_publisher_gui"/>

	<!-- Combine joint values -->
	<node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_publisher"/>

	<!-- Show in Rviz -->
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find gripper_description)/rviz/rg3v2.rviz"/>

</launch>