<launch>
	<!-- Launch Your robot arms launch file which loads the robot in Gazebo and spawns the controllers -->
	<include file = "$(find tangs_description)/launch/tangs_xacro_lab.launch" />

	<!-- Launch Moveit Move Group Node -->
	<include file = "$(find tangs_moveit_config)/launch/move_group.launch" />

	<!-- Run Rviz and load the default configuration to see the state of the move_group node -->
	<arg name="use_rviz" default="true" />

	<include file="$(find tangs_moveit_config)/launch/moveit_rviz.launch" if="$(arg use_rviz)">
		<arg name="rviz_config" value="$(find tangs_moveit_config)/launch/moveit.rviz"/>
	</include>

</launch>
